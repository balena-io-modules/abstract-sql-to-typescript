{
  "name": "@balena/abstract-sql-to-typescript",
  "version": "1.4.2",
  "description": "A translator for abstract sql into typescript types.",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "scripts": {
    "pretest": "npm run lint && npm run prepare",
    "test": "mocha",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"; tsc --project ./tsconfig.build.json",
    "lint": "balena-lint --typescript --fix src test && tsc --noEmit"
  },
  "repository": "https://github.com/balena-io-modules/abstract-sql-to-typescript.git",
  "author": "",
  "dependencies": {
    "@balena/abstract-sql-compiler": "7.26.1-build-web-resource-6-a260e61ac59cff84333ccfe727e2ee53762e5e7c-1",
    "@balena/odata-to-abstract-sql": "5.9.3-build-web-resource-3-0e741854508a48b4a1589d8f8809d5d4ebe22bfe-1",
    "@types/node": "^12.20.55",
    "common-tags": "^1.8.2"
  },
  "devDependencies": {
    "@balena/lint": "^6.2.1",
    "@types/chai": "^4.3.4",
    "@types/common-tags": "^1.8.1",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --typescript --fix"
    ]
  },
  "mocha": {
    "require": "ts-node/register/transpile-only",
    "_": "test/**/*.ts"
  },
  "versionist": {
    "publishedAt": "2023-02-15T13:37:18.265Z"
  }
}
