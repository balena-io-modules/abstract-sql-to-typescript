{
  "name": "@balena/abstract-sql-to-typescript",
  "version": "0.0.0",
  "description": "A translator for abstract sql into typescript types.",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "scripts": {
    "pretest": "npm run lint && npm run prepare",
    "test": "mocha",
    "prepare": "tsc --project ./tsconfig.build.json",
    "lint": "balena-lint --typescript --fix src test && tsc --noEmit"
  },
  "repository": "https://github.com/balena-io-modules/abstract-sql-to-typescript.git",
  "author": "",
  "dependencies": {
    "@balena/abstract-sql-compiler": "^7.0.3",
    "@balena/odata-to-abstract-sql": "^5.3.0",
    "@types/node": "^12.12.62",
    "common-tags": "^1.8.0"
  },
  "devDependencies": {
    "@balena/lint": "^5.2.0",
    "@types/chai": "^4.2.12",
    "@types/common-tags": "^1.8.0",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.1.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --typescript --fix"
    ]
  },
  "mocha": {
    "require": "ts-node/register/transpile-only",
    "_": "test/**/*.ts"
  }
}
