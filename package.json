{
  "name": "@balena/abstract-sql-to-typescript",
  "version": "1.2.1",
  "description": "A translator for abstract sql into typescript types.",
  "main": "out/index.js",
  "types": "out/index.d.ts",
  "scripts": {
    "pretest": "npm run lint && npm run prepare",
    "test": "mocha",
    "prepare": "tsc --project ./tsconfig.build.json",
    "lint": "balena-lint --typescript --fix src test && tsc --noEmit"
  },
  "repository": "https://github.com/balena-io-modules/abstract-sql-to-typescript.git",
  "author": "",
  "dependencies": {
    "@balena/abstract-sql-compiler": "^7.19.1",
    "@balena/odata-to-abstract-sql": "^5.4.9",
    "@types/node": "^12.20.47",
    "common-tags": "^1.8.2"
  },
  "devDependencies": {
    "@balena/lint": "^6.2.0",
    "@types/chai": "^4.3.0",
    "@types/common-tags": "^1.8.1",
    "@types/mocha": "^8.2.3",
    "chai": "^4.3.6",
    "husky": "^4.3.8",
    "lint-staged": "^12.3.7",
    "mocha": "^9.2.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --typescript --fix"
    ]
  },
  "mocha": {
    "require": "ts-node/register/transpile-only",
    "_": "test/**/*.ts"
  },
  "versionist": {
    "publishedAt": "2022-10-14T16:27:10.695Z"
  }
}
